<!DOCTYPE html>
<html lang="zxx">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no, minimum-scale=1.0, maximum-scale=1.0" />
    <meta name="description" content="{{ app_name }}">
    <meta name="keywords" content="{{ app_name }}">

    <title>{% block title %}Welcome{% endblock %}</title>
     {% block stylesheets %}
         <link rel="shortcut icon" type="image/x-icon" href="{{ asset('app/image/favicon.ico') }}">
     {% endblock %}
    </head>

    <body>
    <div class="layout-wrapper layout-content-navbar  ">
        <div class="layout-container">
            {% include '_widgets/admin/sidebar.html.twig' %}
            <!-- Layout container -->
                <div class="layout-page">
                    {% include '_widgets/admin/header.html.twig' %}
                    <!-- Content and footer -->
                    <div class="content-wrapper">
                    {% block body %}{% endblock %}

                    {% include '_widgets/admin/footer.html.twig' %}

                    <div class="content-backdrop fade"></div>
                    </div>
                </div>
        </div>
        <!-- Overlay -->
        <div class="layout-overlay layout-menu-toggle"></div>
        <!-- Drag Target Area To SlideIn Menu On Small Screens -->
        <div class="drag-target"></div>
    </div>

    {% block javascripts %}
        {% block importmap %}{{ importmap('admin') }}{% endblock %}
    {% endblock %}
    </body>
</html>
